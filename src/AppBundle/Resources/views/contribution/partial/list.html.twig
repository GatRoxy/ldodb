{% if contributions | length > 0 %}    
    <h2>Contributions</h2>
    <ul>
        {% for contribution in contributions %}
        <li>
            <a href="{{ path("contribution_show", {"id":contribution.id}) }}">
                {% block callback %}
                    {{ contribution}}
                {% endblock %}
            </a>
        </li>
        {% endfor %} 
    </ul>
    {% endif %}