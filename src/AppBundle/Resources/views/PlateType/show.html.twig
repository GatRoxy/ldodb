{% extends 'base.html.twig' %}

 {% block pageheader %} 
    <h1>PlateType</h1>
 {% endblock %} 

{% block body %}

    <div class="btn-toolbar">
    <div class="btn-group pull-right">
        <a href="{{ path('plate_type_index') }}" class="btn btn-default">
            <span class="glyphicon glyphicon-arrow-left"></span> Back
        </a>
        <a href="{{ path('plate_type_edit', { 'id': plateType.id }) }}" class="btn btn-default">
